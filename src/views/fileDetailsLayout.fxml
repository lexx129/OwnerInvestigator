<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="124.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ChooseFileCtrl">
   <children>
      <Label layoutX="14.0" layoutY="14.0"
             text="Укажите путь к исследуемому объекту: " />
      <TextField fx:id="input" layoutX="14.0" layoutY="42.0"
                 prefHeight="25.0" prefWidth="303.0"
                 promptText="Путь к объекту" />
      <Button fx:id="openButton" layoutX="331.0" layoutY="42.0" mnemonicParsing="false"
              onAction="#handleFile" text="Обзор.." underline="true" />
      <Button fx:id="cancelButton" cancelButton="true" layoutX="14.0" layoutY="85.0" mnemonicParsing="false"
              onAction="#handleCancel" text="Отмена" />
      <Button fx:id="okButton" defaultButton="true" disable="true" layoutX="90.0" layoutY="85.0" mnemonicParsing="false"
              onAction="#handleOk" text="Далее" />
   </children>
</AnchorPane>
